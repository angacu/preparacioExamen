{% extends 'base.html.twig' %}
{% block body %}
<ul id="productos">
  {% for product in productos %}

  <table border="1px">
    <tr>
      <td><b>ID</b></td>
      <td><b>NOMBRE</b></td>
      <td><b>PRECIO</b></td>
      <td><b>IVA</b></td>
    </tr>
    <tr>
      <td> <a href="{{ path('producto_id',{'id':product.id}) }}">{{product.id}} </a></td>
      <td> {{product.nombre}} </td>
      <td> {{product.precio}} </td>
      <td> {{product.iva}} </td>
      <br>
    </tr>
  </table>



  {% endfor %}


</ul>
{% endblock %}
